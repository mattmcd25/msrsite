{
  "name": "msrna",
  "version": "0.1.0",
  "description": "Web application for managing MSR Namibia's member database",
  "devDependencies": {
    "concurrently": "^3.5.1",
    "react-scripts": "1.1.1"
  },
  "dependencies": {
    "express": "^4.16.3",
    "@risingstack/protect": "^1.2.0",
    "mssql": "^4.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2"
  },
  "author": "mattmcd25 & wittenber0",
  "scripts": {
    "start": "concurrently --kill-others --names \"server,client\" \"npm run server\" \"npm run client\"",
    "build+run": "npm run build && npm run server",
    "server": "node src/server.js",
    "client": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "proxy": "http://localhost:3005"
}
